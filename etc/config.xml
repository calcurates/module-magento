<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <calcurates>
                <active>0</active>
                <showmethod>1</showmethod>
                <api_url>https://api.calcurates.com</api_url>
                <display_rates_with_tax>1</display_rates_with_tax>
                <model>Calcurates\ModuleMagento\Model\Carrier</model>
                <calcurates_token backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
                <debug>0</debug>
                <other_shipping_methods_action>2</other_shipping_methods_action>
                <api_get_rates_timeout>10</api_get_rates_timeout>
            </calcurates>
        </carriers>
    </default>
</config>
